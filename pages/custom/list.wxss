/* pages/custom/list.wxss */
page {
  background: #fff;
}
.task-box{
  padding: 30rpx 0;
}


.van-search__content{
  box-shadow: 0rpx 0rpx 20rpx 0rpx rgba(0,31,102,0.06);
  border-radius: 10rpx;
}
.apply-item {
  height:150rpx;
  display: flex;
  padding-right:30rpx;
  padding-top:40rpx;
  border-radius: 15rpx;
  margin-left:30rpx;
  border-bottom:2rpx solid #e6e6e6;
}

.box-right {
  flex: 1;
  display: flex;
  flex-direction: row;
  font-size: 11pt;
}

.box-right-item1 {
  flex:1;
}

.box-right-item1 .title {
  font-size:32rpx;
  color:#333;
}
.list-y{
  min-width:100rpx;
  font-size:28rpx;
  color:#FA9A00;
  margin-top:20rpx;
}
.type-tag{
  height:54rpx;
  padding:0 15rpx;
  background-color: #004ce5;
  line-height: 54rpx;
  color:#fff;
  text-decoration: underline;
  border-radius:10rpx;
  letter-spacing: 1px;
}
.list-b{
  color:#666666;
  font-size:28rpx;
  margin-top:20rpx;
}
.btn-d .van-button{
    height:60rpx;
    font-size:24rpx;
    text-decoration: underline;
    width:180rpx;
    border-radius:6rpx;
}

view .van-search__content {
  background: #fff;
}

.bottom-line {
  color: #999999;
  font-size: 24rpx;
  line-height: 100rpx;
  text-align: center
}
